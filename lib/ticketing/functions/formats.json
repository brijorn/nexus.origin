[
    {
        "name": "{{user}}",
        "changeto": "message.author",
        "description": "Mentions the user"
    },
    {
        "name": "{{user.name}}",
        "changeto": "message.author.username",
        "description": "Changes to the user's nickname"
    },
    {
        "name": "{{user.avatar}}",
        "changeto": "message.author.avatarURL()",
        "description": "Changes to user's avatar"
    },
    {
        "name": "{{count}}",
        "changeto": "parseInt(panel.ticket_count) + 1",
        "description": "Changes to the amount of tickets there are"
    },
    {
        "name": "{{ticket.title}}",
        "changeto": "(info.title) ? info.title : ''",
        "description": "Changes to the title the user gave when creating the ticket"
    },
    {
        "name": "{{creation.date}}",
        "changeto": "(type === 'claim') ? 'Apple' : moment().format('MMMM Do, YYYY hh:mm a')",
        "description": "When the ticket was creation, in terms of claim system it is when the user first ran the command"
    }
]